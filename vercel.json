{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/", "dest": "/api/index.js" },
    { "src": "/style.css", "dest": "/site/style.css" },
    { "src": "/main.js", "dest": "/site/main.js" },
    { "src": "/images/(.*)", "dest": "/site/images/$1" }
  ]
}
